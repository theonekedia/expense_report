<!DOCTYPE html>
<html>
<head>
  <title>ExpenseMongo</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
    <div id="contentWrapper">
      <div class="blue-b">
        <%= render :partial => "layouts/header_nav" %>
      </div>
      <% if current_user %>
      <%= render :partial => "layouts/actions_nav" %>
      <% end %>
      <div class="mc-a">
        <%= yield %>
      </div>
    </div>
  </body>
</html>
